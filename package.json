{
    "name": "gthibaud-icons-generator",
    "version": "0.0.0",
    "license": "SEE LICENSE IN ./LICENSE",
    "author": "Gr√©goire Thibaud <gregoire@creastel.com>",
    "private": false,
    "main": "./lib/index.tsx",
    "types": "./lib/index.d.tsx",
    "files": [
        "lib/**/*"
    ],
    "keywords": [
        "icons",
        "gthibaud"
    ],
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:gthibaud/icons.git",
        "directory": "/"
    },
    "scripts": {
        "dev": "rm -rf ./lib && tsc && node lib/index.js && cd packages/react && yarn build",
        "start": "node build/index.js",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
        "lint": "tslint -p tsconfig.json",
        "build": "rm -rf ./lib && tsc",
        "checkPrepare": "yarn test && yarn format && yarn lint && yarn build",
        "release": "yarn version --patch && npm publish --tag latest",
        "releaseMajor": "yarn version --patch --major && npm publish --tag latest",
        "preversion": "yarn format && yarn lint && yarn build",
        "prepare": "husky install"
    },
    "prettier": "@owlgrid-dev/prettier-config",
    "lint-staged": {
        "*.{json,md,css}": [
            "prettier --write"
        ],
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "tslint -p tsconfig.json"
        ]
    },
    "devDependencies": {
        "@owlgrid-dev/prettier-config": "^0.0.6",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.2.5",
        "@types/prettier": "^2.7.3",
        "fs": "^0.0.1-security",
        "fs-extra": "^11.1.1",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.1",
        "node-html-parser": "^6.1.5",
        "prettier": "^2.8.8",
        "prettier-plugin-organize-imports": "^3.2.2",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.9.4",
        "uppercamelcase": "^3.0.0"
    },
    "dependencies": {}
}